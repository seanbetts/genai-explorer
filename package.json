{
  "name": "genai-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:personal": "NEXT_PUBLIC_BRAND=personal next dev --turbopack",
    "dev:omg": "NEXT_PUBLIC_BRAND=omg next dev --turbopack",
    "dev:personal:3000": "NEXT_PUBLIC_BRAND=personal next dev --turbopack -p 3000",
    "dev:omg:3001": "NEXT_PUBLIC_BRAND=omg next dev --turbopack -p 3001",
    "dev:both": "concurrently \"npm run dev:personal:3000\" \"npm run dev:omg:3001\"",
    "build": "NODE_ENV=production next build",
    "build:personal": "NODE_ENV=production NEXT_PUBLIC_BRAND=personal next build",
    "build:omg": "NODE_ENV=production NEXT_PUBLIC_BRAND=omg next build",
    "start": "next start",
    "lint": "next lint",
    "images:convert": "node scripts/convert-to-webp.js",
    "images:optimize": "node scripts/convert-to-webp.js --directory=public/images --quality=85 --delete",
    "images:optimize:node": "node scripts/optimize-images-node.js --directory=public/images --quality=85 --delete",
    "images:optimize:netlify": "npm install --no-save sharp glob && node scripts/optimize-images-node.js --directory=public/images --quality=85 --delete",
    "prebuild": "npm run images:optimize || npm run images:optimize:node || npm run images:optimize:netlify || echo 'Image optimization skipped'"
  },
  "dependencies": {
    "bootstrap-icons": "^1.11.3",
    "next": "15.3.0",
    "papaparse": "^5.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-config-next": "^15.3.1",
    "glob": "^7.2.0",
    "sharp": "^0.32.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
